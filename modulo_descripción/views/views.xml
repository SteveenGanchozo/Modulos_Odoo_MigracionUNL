<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="modulo_descripción.list">
      <field name="name">Modulo descripción list</field>
      <field name="model">reg.descripcion</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name" string="Descripción Extra"/>
        </tree>
      </field>
    </record>


    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="modulo_descripción.action_window">
      <field name="name">Descripción Extra</field>
      <field name="res_model">reg.descripcion</field>
      <field name="view_mode">tree,form</field>
    </record>


    <!-- <menuitem name="Descripción Extra" id="modulo_descripción.menu_1_list" parent="" action="modulo_descripción.action_window"/> -->
  </data>
</odoo>